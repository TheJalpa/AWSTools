variable "awsrolename" {
    description = "name of the role that will be used"
    default = "rolenamehere"
}

variable "servicename" {
    description = "name of the service this role is for"
    default = "servicenamehere"
}

variable "departmentname" {
    description = "name of the department using this role"
    default = "departmentnamehere"
}